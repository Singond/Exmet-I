load "../plotsettings.gp"
set border 15
set tmargin at screen 0.95
set bmargin at screen 0.55
set lmargin at screen 0.11
set xtics in mirror
set ytics in mirror
set key off
set log x
set format x ''
set ylabel '$|\sparam|\,[\si{\decibel}]$'
set format y '%4g'  # Unify width